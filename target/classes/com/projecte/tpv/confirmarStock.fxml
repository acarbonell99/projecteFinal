<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.text.Text?>

<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Segur que vols confirmar l'estoc">
            <VBox.margin>
                <Insets bottom="32.0" left="32.0" right="32.0" top="32.0" />
            </VBox.margin>
        </Text>
        <HBox alignment="BOTTOM_RIGHT">
            <children>
                <Button mnemonicParsing="false" prefHeight="30.0" prefWidth="40.0" text="Ok">
                    <HBox.margin>
                        <Insets bottom="4.0" left="4.0" right="4.0" top="4.0" />
                    </HBox.margin>
                </Button>
                <Button mnemonicParsing="false" prefHeight="30.0" prefWidth="40.0" text="X">
                    <HBox.margin>
                        <Insets bottom="4.0" left="4.0" right="4.0" top="4.0" />
                    </HBox.margin>
                </Button>
            </children>
        </HBox>
    </children>
    <padding>
        <Insets bottom="16.0" left="16.0" right="16.0" top="16.0" />
    </padding>
</VBox>
