<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane alignment="CENTER" stylesheets="@css/estils.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.projecte.tpv.PostvendaController">
  <columnConstraints>
      <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
    <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" />
    <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" />
      <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
  </columnConstraints>
  <rowConstraints>
      <RowConstraints minHeight="10.0" prefHeight="30.0" valignment="CENTER" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" valignment="CENTER" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" valignment="CENTER" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" valignment="CENTER" vgrow="SOMETIMES" />
  </rowConstraints>
   <children>
      <Button minHeight="120.0" minWidth="150.0" mnemonicParsing="false" onAction="#toStock" prefHeight="120.0" prefWidth="180.0" style="-fx-border-radius: 16; -fx-background-radius: 16;" text="Stock" GridPane.columnIndex="1" GridPane.rowIndex="1">
         <styleClass>
            <String fx:value="btn" />
            <String fx:value="btn-info" />
         </styleClass></Button>
      <Button minHeight="120.0" minWidth="150.0" mnemonicParsing="false" onAction="#toNeteja" prefHeight="120.0" prefWidth="180.0" style="-fx-border-radius: 16; -fx-background-radius: 16;" text="Neteja" GridPane.columnIndex="2" GridPane.rowIndex="1">
         <styleClass>
            <String fx:value="btn" />
            <String fx:value="btn-info" />
         </styleClass></Button>
      <Button minHeight="120.0" minWidth="150.0" mnemonicParsing="false" onAction="#toRecompte" prefHeight="120.0" prefWidth="180.0" style="-fx-border-radius: 16; -fx-background-radius: 16;" text="Recompte Caixa" GridPane.columnIndex="1" GridPane.rowIndex="2">
         <styleClass>
            <String fx:value="btn" />
            <String fx:value="btn-info" />
         </styleClass></Button>
      <Button minHeight="120.0" minWidth="150.0" mnemonicParsing="false" onAction="#toPersonal" prefHeight="120.0" prefWidth="180.0" style="-fx-border-radius: 16; -fx-background-radius: 16;" text="Personal" GridPane.columnIndex="2" GridPane.rowIndex="2">
         <styleClass>
            <String fx:value="btn" />
            <String fx:value="btn-info" />
         </styleClass></Button>
      <Button fx:id="btnBack" mnemonicParsing="false" onAction="#backInici" prefWidth="100.0" prefHeight="30.0" text="Back" GridPane.halignment="LEFT" GridPane.valignment="TOP">
         <styleClass>
             <String fx:value="btn" />
             <String fx:value="btn-danger" />
             <String fx:value="btn-sm" />
         </styleClass></Button>
   </children>
   <padding>
      <Insets bottom="16.0" left="16.0" right="16.0" top="16.0" />
   </padding>
</GridPane>